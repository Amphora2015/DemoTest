<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="ConfirmManager.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
    <sectionGroup name="codemesh">
      <sectionGroup name="runtime">
        <section name="Loader" type="System.Configuration.SingleTagSectionHandler" />
        <section name="Default.JvmSettings" type="System.Configuration.NameValueSectionHandler" />
        <section name="Default.Options" type="System.Configuration.NameValueSectionHandler" />
        <section name="Default.XOptions" type="System.Configuration.NameValueSectionHandler" />
      </sectionGroup>
    </sectionGroup>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" requirePermission="false" />
  </configSections>
  <system.net>
    <connectionManagement>
      <add address="*" maxconnection="10" />
    </connectionManagement>
  </system.net>
  <appSettings>
    <add key="log4net.Internal.Debug" value="true" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  <log4net>
    <!-- Define some output appenders -->
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="${APPDATA}/../Local/Amphora/CNF/Log/ConfirmManager.log" />
      <param name="AppendToFile" value="true" />
      <param name="MaxSizeRollBackups" value="3" />
      <param name="MaximumFileSize" value="20MB" />
      <param name="RollingStyle" value="Size" />
      <param name="StaticLogFileName" value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%date  [%thread] %logger %message%newline" />
      </layout>
    </appender>
    <appender name="DebugAppender" type="log4net.Appender.DebugAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="ALL" />
      <appender-ref ref="RollingLogFileAppender" />
      <appender-ref ref="DebugAppender" />
    </root>
  </log4net>
  <connectionStrings />
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_Counterparty" sendTimeout="00:02:00"/>
        <binding name="BasicHttpBinding_ConfirmationsManager" sendTimeout="00:02:00"/>
        <binding name="BasicHttpBinding_GetDocument" sendTimeout="00:05:00" maxBufferSize="999999" maxReceivedMessageSize="999999"/>
        <binding name="BasicHttpBinding_VaultSvc" sendTimeout="00:05:00" maxBufferSize="999999" maxReceivedMessageSize="999999"/>
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://0.0.0.0:11111/ConfirmationsManager.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ConfirmationsManager" contract="SvcRef_ConfirmationsManager.ConfirmationsManager" name="BasicHttpBinding_ConfirmationsManager" />
      <endpoint address="http://0.0.0.0:11111/Counterparty.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_Counterparty" contract="SvcRef_Counterparty.Counterparty" name="BasicHttpBinding_Counterparty" />
      <endpoint address="http://0.0.0.0:11111/GetDocument.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_GetDocument" contract="SvcRef_GetDocument.GetDocument" name="BasicHttpBinding_GetDocument" />
      <endpoint address="http://0.0.0.0:11111/VaultService" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_VaultSvc" contract="VaultSvcClient.VaultSvc" name="BasicHttpBinding_VaultSvc" />
