<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
         version="3.0">

    <enterprise-beans>
        <session>
            <ejb-name>Confirmation</ejb-name>
            <env-entry>
                <env-entry-name>vaultWebServerUrl</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>http://stfaxgateway/SempraDocumentWebService/SempraDocWS.asmx</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>loginUser</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>SEMPRATRADING\Dealsheet</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>loginPassword</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>@#Vaultsheet</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>dslName</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>AX-Prod</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>repositoryName</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>OPSMANAGER_CONTRACTS</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>repositoryNameJPM</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>OPSMANAGER_CONTRACTS</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>updateContractDb</env-entry-name>
                <env-entry-type>java.lang.Boolean</env-entry-type>
                <env-entry-value>false</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>getContractDb</env-entry-name>
                <env-entry-type>java.lang.Boolean</env-entry-type>
                <env-entry-value>false</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>sempraCompanyId</env-entry-name>
                <env-entry-type>java.lang.Long</env-entry-type>
                <env-entry-value>80681</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>creditStatusUrl</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>http://stappsrv6/creditlib/IsMarginExists.aspx</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>creditMarginUrl</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>http://stappsrv6/creditlib/creditterms.aspx</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>creditMarginToken</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>[MARGIN TOKEN]</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>creditServiceUser</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>opsmargin</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>traderWebUrl</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>http://stappsrv8:8080/OpsManagerTrader</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>smtpHost</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>st-mailhost</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>smtpPort</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>25</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>vaultFolders</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>
                    S:OPSMANAGER_CONTRACTS,J:OPSMANAGER_CONTRACTS,G:OPSMANAGER_CONTRACTS
                </env-entry-value>
            </env-entry>

        </session>
    </enterprise-beans>

</ejb-jar>