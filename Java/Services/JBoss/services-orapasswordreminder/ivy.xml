<ivy-module version="2.0"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">

    <info organisation="cnf" module="cnf-OraPasswordReminderService"/>

    <configurations>
        <include file="${project.basedir}/ivy-configurations.xml"/>
    </configurations>

    <publications>
        <artifact type="jar" conf="master"/>
    </publications>

    <dependencies defaultconf="default">
        <dependency org="commons-httpclient" name="commons-httpclient" rev="3.1"/>

        <dependency name="cnf-DBInfoService" rev="latest.integration" changing="true"/>
        <dependency name="cnf-DAOCommon" rev="latest.integration" changing="true"/>
        <dependency name="cnf-CommonServiceLib" rev="latest.integration" changing="true"/>
    </dependencies>

</ivy-module>
